{
  "name": "web-scrapper",
  "version": "1.0.0",
  "description": "This is a basic node cheerio project setup with typescript.",
  "author": "Ion Chirita",
  "main": "dist/index.js",
  "license": "MIT",
  "homepage": "https://github.com/ionutbv16/node-webcrawler-typescript#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionutbv16/node-webcrawler-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/ionutbv16/node-webcrawler-typescript/issues"
  },
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpileOnly --ignore-watch node_modules --no-notify src/index.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.3",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "ts-node-dev": "^1.0.0-pre.49",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  }
}
